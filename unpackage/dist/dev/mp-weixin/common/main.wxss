/*每个页面公共css */
page {
  background-color: #f1f1f1;
}
page .exclass {
    background-color: #f4605e;
}
page .inclass {
    background-color: #62b8fd;
}
page .operating {
    width: 500rpx;
    padding: 20rpx 30rpx;
    min-height: 200rpx;
    border-radius: 20rpx;
    text-align: center;
    background-color: #fff;
}
page .operating > view {
      height: 100rpx;
      line-height: 100rpx;
      text-align: center;
}
page .operating > view:nth-child(1) {
      border-bottom: 2rpx solid #c0c0c0;
}
page .nodata {
    width: 100%;
    margin: 20% 0;
    text-align: center;
}
page .swipaction {
    width: 70%;
    height: 95%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
            justify-content: space-around;
}
page .swipaction view {
      min-width: 28%;
      text-align: center;
      padding: 0 8%;
      color: #333;
}
page .swipaction,
    page .swipaction .exbor {
      color: #f4605e;
      border-bottom: 6rpx solid #f4605e;
}
page .swipaction .inbor {
      color: #62b8fd;
      border-bottom: 6rpx solid #62b8fd;
}
page .swipaction .borbut {
      color: #007aff;
      border-bottom: 6rpx solid #007aff;
}
page .butbox {
    width: 100%;
    height: 100rpx;
}
page .addtypebut {
    position: fixed;
    width: 50%;
    left: 25%;
    bottom: 30rpx;
    border-radius: 60rpx;
    background-color: #007aff;
    color: #fff;
}

