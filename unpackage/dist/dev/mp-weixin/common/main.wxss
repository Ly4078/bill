/*每个页面公共css */
page {
  background-color: #f1f1f1;
}
page .exclass {
    background-color: #4cd964;
}
page .inclass {
    background-color: #007aff;
}
page .operating {
    width: 500rpx;
    padding: 20rpx 30rpx;
    min-height: 200rpx;
    border-radius: 20rpx;
    text-align: center;
    background-color: #fff;
}
page .operating > view {
      height: 100rpx;
      line-height: 100rpx;
      text-align: center;
}
page .operating > view:nth-child(1) {
      border-bottom: 2rpx solid #c0c0c0;
}
page .nodata {
    width: 100%;
    margin: 20% 0;
    text-align: center;
}

